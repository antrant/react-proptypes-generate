{
    "name": "react-proptypes-prod",
    "displayName": "react-propTypes",
    "description": "自动生成propTypes",
    "version": "1.0.0-alpha",
    "publisher": "suming",
    "author": "suming",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/dpzxsm/react-proptypes/issues"
    },
    "homepage": "https://github.com/dpzxsm/react-proptypes",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.propTypesGenerate"
    ],
    "main": "./extension",
    "bin": {
        "web": "./bin/cli.js"
    },
    "contributes": {
        "commands": [
            {
                "command": "extension.propTypesGenerate",
                "title": "PropTypes Generate"
            }
        ],
        "keybindings": [
            {
                "command": "extension.propTypesGenerate",
                "key": "shift+ctrl+alt+p",
                "mac": "shift+cmd+alt+p"
            }
        ],
        "configuration": {
            "type" : "object",
            "title" : "ReactPropTypes configuration",
            "properties": {
                "propTypes.autoImport": {
                    "type": "string",
                    "default": "Disabled",
                    "description": "Auto import or require PropTypes module"
                },
                "propTypes.codeStyle": {
                    "type": "string",
                    "default": "ES6",
                    "description": "PropTypes Generate Style"
                }
            }
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "compile": "node ./node_modules/vscode/bin/compile",
        "start": "code --extensionDevelopmentPath=/Users/user/Projects/react-proptypes"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "bluebird": "^3.5.3",
        "flow-parser": "^0.86.0",
        "recast": "^0.16.0"
    }
}
